{
  "name": "optio-api",
  "version": "1.0.5",
  "engines": {
    "node": "12.10.0"
  },
  "private": true,
  "scripts": {
    "heroku-postbuild": "run-s clean build:server build:heroku:client",
    "npm:install:dev": "npm install --only=dev",
    "npm:install": "npm install",
    "cd:client": "cd client",
    "start": "npm run prod",
    "start-mongodb-on-mac": "mongod --config /usr/local/etc/mongod.conf",
    "start-client": "npm run --prefix client start",
    "combo-start": "./node_modules/.bin/npm-run-all -p dev start-client",
    "comqbo-start-all": "./node_modules/.bin/run-p start-mongodb-on-mac combo-start",
    "server:prod": "node dist/bin/www.js",
    "server:dev": "./node_modules/.bin/nodemon src/bin/www.js  --exec babel-node ./bin/www.js",
    "clean": "rm -rf dist",
    "kill-node": "killall node",
    "build:server": "npx babel src --out-dir dist --ignore src/helpers --ignore src/test",
    "build:client": "npm run --prefix client build",
    "build:heroku:client": "cd client && npm install && npm run build",
    "build": "run-s clean build:server cd:client build:client",
    "dev": "NODE_ENV=dev npm-run-all -s server:dev",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require @babel/register --exit --timeout 30000",
    "prod": "NODE_ENV=production run-s server:prod",
    "seedDb": "node -r @babel/register 'src/helpers/seeddb.js' ",
    "seed-dev": "NODE_ENV=dev run-s seedDb",
    "seed-remote": "NODE_ENV=production run-s seedDb",
    "seed-test": "NODE_ENV=test run-s seedDb "
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@material/select": "^3.1.0",
    "aws-sdk": "^2.524.0",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-busboy": "0.0.2",
    "cookie-parser": "~1.4.4",
    "core-util-is": "^1.0.2",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "express-fileupload": "^1.1.5",
    "helmet": "^3.21.1",
    "http-errors": "~1.6.3",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongo-seeding-cli": "^3.3.0",
    "mongoose": "^5.6.10",
    "mongoose-type-url": "^1.0.6",
    "morgan": "~1.9.1",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "redux": "^4.0.4",
    "sharp": "^0.23.1",
    "socket.io": "^2.3.0",
    "uuid": "^3.3.3",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.17.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0",
    "prettier": "1.18.2",
    "redux-mock-store": "^1.5.3"
  }
}
